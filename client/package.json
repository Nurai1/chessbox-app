{
	"name": "compliance-tools-frontend",
	"private": true,
	"version": "0.1.0",
	"engines": {
		"node": "^14.18.0 || ^16 || >=18",
		"pnpm": "^8.2.0"
	},
	"scripts": {
		"build": "pnpm lint && vite build",
		"start": "vite --open",
		"generate-types": "pnpm openapi-typescript ../server/swagger_output.json -o ./src/types/generated.ts",
		"prepare": "pnpm simple-git-hooks",
		"preview": "vite preview",
		"format": "prettier -uw --cache --ignore-path .gitignore .",
		"run-tsc": "tsc",
		"run-eslint": "eslint --ext .ts,.tsx .",
		"lint": "run-p run-tsc run-eslint",
		"storybook": "start-storybook -p 6006",
		"build-storybook": "build-storybook"
	},
	"dependencies": {
		"@reduxjs/toolkit": "^1.9.5",
		"dayjs": "^1.11.7",
		"openapi-fetch": "^0.4.0",
		"openapi-typescript": "^6.2.8",
		"react": "^18.2.0",
		"react-dnd": "^16.0.1",
		"react-dnd-html5-backend": "^16.0.1",
		"react-dom": "^18.2.0",
		"react-redux": "^8.1.1",
		"react-router-dom": "^6.7.0",
		"react-virtualized-auto-sizer": "^1.0.20",
		"react-window": "^1.8.9",
		"react-window-infinite-loader": "^1.0.9",
		"remeda": "^1.24.0",
		"tailwind-merge": "^1.9.1",
		"tailwind-variants": "^0.0.30",
		"type-fest": "^3.5.0",
		"usehooks-ts": "^2.9.1",
		"zod": "^3.20.2"
	},
	"devDependencies": {
		"@babel/core": "^7.20.7",
		"@mdx-js/react": "^2.2.1",
		"@storybook/addon-actions": "^6.5.15",
		"@storybook/addon-essentials": "^6.5.15",
		"@storybook/addon-interactions": "^6.5.15",
		"@storybook/addon-links": "^6.5.15",
		"@storybook/builder-vite": "^0.2.6",
		"@storybook/react": "^6.5.15",
		"@storybook/testing-library": "^0.0.13",
		"@tailwindcss/forms": "^0.5.3",
		"@types/node": "^18.11.17",
		"@types/react": "^18.0.25",
		"@types/react-dom": "^18.0.8",
		"@types/react-virtualized-auto-sizer": "^1.0.1",
		"@types/react-window": "^1.8.5",
		"@types/react-window-infinite-loader": "^1.0.6",
		"@typescript-eslint/eslint-plugin": "^5.43.0",
		"@typescript-eslint/parser": "^5.43.0",
		"@vitejs/plugin-react": "^2.2.0",
		"autoprefixer": "^10.4.13",
		"babel-loader": "^8.3.0",
		"eslint": "^8.27.0",
		"eslint-config-airbnb": "^19.0.4",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-config-airbnb-typescript": "^17.0.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-jsx-a11y": "^6.6.1",
		"eslint-plugin-react": "^7.31.10",
		"eslint-plugin-react-hooks": "^4.6.0",
		"husky": "^8.0.3",
		"lint-staged": "^13.1.0",
		"npm-run-all": "^4.1.5",
		"postcss": "^8.4.19",
		"prettier": "~2.7.1",
		"prettier-plugin-tailwindcss": "^0.1.13",
		"rollup-plugin-visualizer": "^5.8.3",
		"simple-git-hooks": "^2.8.1",
		"tailwindcss": "^3.2.7",
		"typescript": "^4.8.4",
		"vite": "^3.2.4",
		"vite-plugin-checker": "^0.5.3",
		"vite-plugin-svgr": "^2.4.0",
		"vite-tsconfig-paths": "^4.0.3"
	},
	"browserslist": [
		"last 2 chrome versions",
		"last 2 firefox versions"
	],
	"simple-git-hooks": {
		"pre-commit": "cd client && pnpm lint-staged"
	}
}
