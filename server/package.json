{
  "name": "chessbox-app-server",
  "private": true,
  "version": "1.0.0",
  "license": "MIT",
  "exports": "./dist/app.js",
  "scripts": {
    "start": "nodemon index.ts",
    "swagger": "ts-node generateSwaggerApi.ts && npm run generate-client-types && npm start",
    "start-js": "node index.js",
    "lint": "eslint --ext .ts,.js .",
    "build": "npm run clean && npx tsc && npm run copy-files",
    "clean": "rm -rf ./dist",
    "copy-files": "cp ./Dockerfile ./dist && cp .dockerignore ./dist && cp ./package.json ./dist && cp ./package-lock.json ./dist",
    "buildx": "cd dist && docker buildx build --platform linux/amd64,linux/arm64 --push -t rg.pl-waw.scw.cloud/funcscweclectium0iuzdi5t/chessbox-server . && cd ../",
    "generate-client-types": "npx openapi-typescript ./swagger_output.json -o ../client/src/types/generated.ts"
  },
  "dependencies": {
    "accesscontrol": "^2.2.1",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^4.1.4",
    "mongoose": "^6.9.2",
    "node-fetch": "^2.6.11",
    "serverless-http": "^3.2.0",
    "typescript": "^4.5.2",
    "zod": "^3.20.6"
  },
  "devDependencies": {
    "@types/node-fetch": "^2.6.4",
    "@types/bcrypt": "^5.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "eslint": "^8.3.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.0.0",
    "mongoose-to-swagger": "^1.4.0",
    "nodemon": "^2.0.15",
    "openapi-typescript": "^6.2.8",
    "prettier": "^2.5.0",
    "swagger-autogen": "^2.23.2",
    "swagger-ui-express": "^4.6.3",
    "ts-node": "^10.9.1"
  }
}
