{
  "name": "chessbox-app-server",
  "private": true,
  "version": "1.0.0",
  "license": "MIT",
  "type": "module",
  "exports": "./dist/app.js",
  "scripts": {
    "start": "nodemon index.ts",
    "lint": "eslint --ext .ts,.js .",
    "build": "npm i && npm run clean && npx tsc && npm i --production && npm run copy-files && zip -r function.zip dist/ && npm run sendToAws",
    "sendToAws": "aws lambda update-function-code --function-name chessbox-app --zip-file fileb://function.zip",
    "clean": "rm -rf ./dist",
    "copy-files": "cp -R ./public ./dist/public && cp -R ./node_modules/ ./dist/node_modules"
  },
  "dependencies": {
    "accesscontrol": "^2.2.1",
    "bcrypt": "^5.0.1",
    "dotenv": "^10.0.0",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^4.1.4",
    "mongoose": "^6.9.2",
    "serverless-http": "^3.2.0",
    "typescript": "^4.5.2",
    "zod": "^3.20.6"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.6",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "eslint": "^8.3.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.0.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.0",
    "ts-node": "^10.9.1"
  }
}
